  "addons": [
    "heroku-postgresql:hobby-dev"
  ],
  "buildpacks": [
    {
      "url": "heroku/go"
    }
  ],
  "env": {
    "NEEDS_AUTH": {
      "required": true
    },
    "TOKEN_PASSWORD": {
